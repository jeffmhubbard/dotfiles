! .Xresources
 
!! --- Colors ---
#include ".Xresources.antsy"

!! --- Fonts ---
Xft.autohint:                   0
Xft.lcdfilter:                  lcddefault
Xft.hintstyle:                  hintslight
Xft.hinting:                    1
Xft.antialias:                  1
Xft.rgba:                       rgb

!! --- Cursor ---
Xcursor.theme:                  "OpenZone Black"
Xcursor.size:                   16

!! --- URxvt ---
URxvt*title:                    URxvt
URxvt*termName:                 rxvt-unicode-256color
URxvt*depth:                    32
URxvt*saveLines:                2500
URxvt*urgentOnBell:             true
URxvt*internalBorder:           3
URxvt*lineSpace:                0
URxvt*scrollBar:                false
URxvt*cursorBlink:              true
URxvt*cursorUnderline:          true
URxvt*colorULMode:              on
URxvt.blurRadius:               2
!URxvt*transparent:              true
!URxvt*shading:                  10
URxvt*font:                     xft:Hack Nerd Font:pixelsize=13:Regular
URxvt*boldFont:                 xft:Hack Nerd Font:pixelsize=13:Regular:Bold
URxvt.italicFont:               xft:Hack Nerd Font:pixelsize=11:Italic
URxvt.boldItalicFont:           xft:Hack Nerd Font:pixelsize=11:Italic:Bold
URxvt*perl-ext-common:          default,matcher
URxvt.url-launcher:             qutebrowser
URxvt.matcher.button:           1

XTerm*font:                     xft:Hack Nerd Font:pixelsize=13:Regular

! vim: set ft=xdefaults :
